<?xml version='1.0' encoding='utf-8'?>
<widget id="com.alpacatech.eaamockup" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Emergency Assistance</name>
    <description> Mobile Version to Capstone Project of Alpaca Tech</description>
    <author href="" email="jdl28110@gmail.com">Juan C. de León Díaz</author>
    <icon src="images/hospital-building.png" gap:role="default"/>
    <content src="index.html" />
    <access origin="*"/>
    <access uri="*" subdomains="true" origin="*" />
    <access uri="http://websocketchat-c9-erreh.c9.io" subdomains="true" origin="http://websocketchat-c9-erreh.c9.io" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="phonegap-version" value="3.1.0" />
    <preference name="KeyboardShrinksView" value="true" />
    <gap:splash src="images/user-marker.png"/>
    <gap:splash src="images/splash-160x220.png" gap:platform="android" gap:density="ldpi"/>
    <gap:splash src="images/splash-220x330.png" gap:platform="android" gap:density="mdpi"/>
    <gap:splash src="images/splash-450x650.png" gap:platform="android" gap:density="hdpi"/>
</widget>
